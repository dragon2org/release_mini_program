{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/sjje/laravel-queue-kafka"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "doctrine/dbal": "^2.9",
        "ellipsesynergie/api-response": "^0.14.2",
        "fideloper/proxy": "~4.0",
        "laravel/framework": "5.8.*",
        "laravel/horizon": "^3.1",
        "laravel/tinker": "~1.0",
        "overtrue/laravel-wechat": "~4.0",
        "predis/predis": "^1.1",
        "rapide/laravel-queue-kafka": "dev-sasl-support",
        "sjje/swaggervel": "3.0.2",
        "symfony/stopwatch": "^4.2",
        "zircote/swagger-php": "2.0.*"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "overtrue/phplint": "^1.1",
        "phpunit/phpunit": "~7.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "tests/Utilities/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
